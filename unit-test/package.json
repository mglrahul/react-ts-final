{
  "name": "max-react-components",
  "version": "1.0.0",
  "description": "typescript based react components with redux as state management",
  "main": "index.js",
  "private": true,
  "scripts": {
    "prepublish": "babel --plugins transform-es2015-modules-umd core --ignore 'test' --out-dir ./build",
    "lint": "tslint ./core",
    "lintfix": "tslint ./src --fix",
    "test": "jest --watch --coverage"
  },
  "author": "Devesh Kaushal",
  "license": "ISC",
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.7.4",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "enzyme": "^2.9.1",
    "enzyme-adapter-react-16": "^1.0.1",
    "expect": "^21.2.1",
    "jest": "^21.2.0",
    "moxios": "^0.4.0",
    "nock": "^9.0.22",
    "q": "^1.5.0",
    "react-addons-test-utils": "^15.0.0",
    "react-polyfill": "0.0.2",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "tslint": "^5.7.0"
  },
  "dependencies": {
    "@types/react": "^16.0.10",
    "@types/react-dom": "^16.0.1",
    "@types/redux-form": "^7.0.6",
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-number-format": "^2.0.4",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "jest": {
    "coverageReporters": [
      "html",
      "text"
    ],
    "testURL": "http://localhost:8080",
    "testRegex": "(/test/unit/*|(test|Spec))\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testEnvironment": "node"
  }
}
